{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/ui/*": [
        "./src/components/ui/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}