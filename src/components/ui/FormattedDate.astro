---
import { undefined } from 'astro:schema';

type Props = {
    pubDate: Date;
}
const {pubDate} = Astro.props
---
<time datetime={pubDate.toISOString()}>
    <strong>
        {
            pubDate.toLocaleDateString(undefined, {
                dateStyle: "long"
            })
        }
    </strong>
</time>